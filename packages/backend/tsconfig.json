{
  "exclude": [
    "./coverage",
    "./dist",
    "__tests__",
    "jest.config.js",
    "*.test.js",
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
